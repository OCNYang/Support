<?xml version="1.0" encoding="utf-8"?>
<!-- 层叠 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <!--<corners android:radius="2dp" />-->
            <gradient
                android:angle="270"
                android:centerColor="@color/h5webview_progress_bar_centercolor"
                android:endColor="@color/h5webview_progress_bar_endcolor"
                android:startColor="@color/h5webview_progress_bar_startcolor" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <!--<corners android:radius="2dp" />-->
                <gradient
                    android:centerColor="@color/h5webview_progress_bar_cover_centercolor"
                    android:endColor="@color/h5webview_progress_bar_cover_endcolor"
                    android:startColor="@color/h5webview_progress_bar_cover_startcolor" />
            </shape>
        </clip>
    </item>

</layer-list>
